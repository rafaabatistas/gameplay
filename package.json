    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
