{
  "expo": {
    "name": "Gameplay",
    "slug": "Gameplay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "gameplay",
    "platforms": ["ios", "android"],
    "description": "Aplicativo onde vocÃª pode criar grupos com seus amigos e agendar jogatinas dos seus games favoritos.",
    "primaryColor": "#0A1033",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1033"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rafaabatistas.gameplay"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.rafaabatistas.gameplay",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1033"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#0A1033"
        }
      ]
    ]
  }
}
